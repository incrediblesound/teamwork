extends layout

block content
  div.container
    h1.text-center Member Home
    div.row
      h2.text-center Welcome #{user.name} of #{group}
      div.col-md-6
        h3 Your missions:
        hr
        - each mission in missions
          p.lead #{mission.name} 
            a(href='/memberview/#{mission.id}' class='btn btn-default') View
